{
  "name": "mediamonks/crawler-bundle",
  "type": "symfony-bundle",
  "description": "Integrates the mediamonks/crawler package into Symfony",
  "keywords": [
    "crawler",
    "seo",
    "dom",
    "spider",
    "robot",
    "prerender",
    "prerender.io"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "mediamonks/crawler": "dev-tac",
    "symfony/framework-bundle": ">= 5.4"
  },
  "require-dev": {
    "matthiasnoback/symfony-dependency-injection-test": "0.*",
    "mockery/mockery": "^0.9.4",
    "phpunit/phpunit": "^5.0",
    "rector/rector": "^0.13.8"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\CrawlerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "": "Tests/Functional/src/"
    }
  },
  "repositories": {
    "survos_scraper": {
      "type": "path",
      "url": "../../projects/crawler"
    }

  },

  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
